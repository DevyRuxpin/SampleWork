{
  "name": "kalishare-backend",
  "version": "1.0.0",
  "description": "Kali Skill Share Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:unit": "jest tests/unit --detectOpenHandles",
    "test:integration": "jest tests/integration --detectOpenHandles",
    "test:e2e": "jest tests/e2e --detectOpenHandles",
    "test:ci": "jest --coverage --ci --detectOpenHandles --verbose",
    "test:debug": "jest --detectOpenHandles --verbose --no-cache",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "@huggingface/inference": "^4.0.6",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.3.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "node-cache": "^5.1.2",
    "pg": "^8.7.3",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.4",
    "eslint": "^8.0.0"
  },
  "keywords": [
    "api",
    "skills",
    "education"
  ],
  "author": "Kali Consulting LLC",
  "license": "MIT"
